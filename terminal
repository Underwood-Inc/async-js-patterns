# Create project directory and initialize
mkdir async-js-patterns
cd async-js-patterns
npm init -y
npm install -D vitepress vue
npm install -D eslint prettier @typescript-eslint/parser @typescript-eslint/eslint-plugin typescript
git init
git add .
git commit -m "Initial commit"
git branch -M master
git remote add origin git@github.com:Underwood-Inc/async-js-patterns.git
git push -u origin master 